{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./",
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,                                /* Create source map files for emitted JavaScript files. */
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}